{
    "name": "vtex-balance-analysis-and-resolution-backend",
    "version": "1.0.0",
    "description": "ferramenta-para-ajudar-na-inconsistencias-de-saldo-vtex",
    "main": "server.js",
    "scripts": {
        "test:integration": "cross-env NODE_ENV=test && jest --projects ./tests --runInBand --verbose --detectOpenHandles --forceExit | npx pino-pretty",
        "test:integration-silent": "cross-env NODE_ENV=test && jest --projects ./tests --runInBand --verbose --detectOpenHandles --forceExit --silent=true",
        "test:unit": "cross-env NODE_ENV=test && jest | npx pino-pretty",
        "test": "npm run test:unit && npm run test:integration",
        "dev": "cross-env NODE_ENV=development && nodemon server.js | npx pino-pretty",
        "debug": "node --debug-brk --inspect ./node_modules/.bin/jest -i",
        "start-prod": "npm start",
        "lint": "eslint ./app/",
        "lint:fix": "eslint ./app/ --ext .js --fix",
        "style:check": "prettier --check \"app/**/*.js\" \"config/**/*.js\" \"tests/**/*.js\"",
        "style:fix": "prettier --write \"app/**/*.js\" \"config/**/*.js\" \"tests/**/*.js\"",
        "coverage": "npm run test -- --coverage --silent=false",
        "single-test": "cross-env NODE_ENV=test && jest -t",
        "start": "node server.js | npx pino-pretty"
    },
    "author": "Henrique Hoinacki",
    "license": "ISC",
    "engines": {
        "npm": ">=9.6.7",
        "node": ">=18.20.4"
    },
    "dependencies": {
        "axios": "^1.7.2",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "camelcase-keys": "^7.0.2",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "csurf": "^1.11.0",
        "date-fns": "^2.30.0",
        "dotenv": "^16.3.1",
        "express": "^4.19.2",
        "express-fileupload": "^1.5.1",
        "express-session": "^1.17.3",
        "express-validation": "^4.1.0",
        "helmet": "^7.0.0",
        "hpp": "^0.2.3",
        "http-status": "^1.7.0",
        "i18n": "^0.15.1",
        "jimp": "^0.22.10",
        "jsonwebtoken": "^9.0.2",
        "jsqr": "^1.4.0",
        "keycloak-connect": "^24.0.5",
        "lodash": "^4.17.21",
        "mongoose": "^8.5.1",
        "mongoose-aggregate-paginate-v2": "^1.0.6",
        "mongoose-paginate-v2": "^1.7.31",
        "multer": "1.4.5-lts.1",
        "node-schedule": "^2.1.1",
        "pino": "^8.15.1",
        "pino-http": "^8.5.0",
        "qs": "^6.14.0"
    },
    "devDependencies": {
        "cross-env": "^7.0.3",
        "eslint": "^8.50.0",
        "jest": "^27.5.1",
        "jest-cli": "^27.5.1",
        "nock": "^13.3.3",
        "nodemon": "^3.1.4",
        "prettier": "^3.3.3",
        "supertest": "^6.3.3"
    }
}
